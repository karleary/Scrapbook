<h1>Listing tag_pictures</h1>

<table>
  <tr>
    <th>Tag</th>
    <th>Picture</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tag_pictures.each do |tag_picture| %>
  <tr>
    <td><%= tag_picture.tag_id %></td>
    <td><%= tag_picture.picture_id %></td>
    <td><%= link_to 'Show', tag_picture %></td>
    <td><%= link_to 'Edit', edit_tag_picture_path(tag_picture) %></td>
    <td><%= link_to 'Destroy', tag_picture, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

